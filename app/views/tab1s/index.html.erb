<h1>Listing tab1s</h1>

<table>
  <thead>
    <tr>
      <th>Productname</th>
      <th>Productparagraph</th>
      <th>Bullet1</th>
      <th>Bullet2</th>
      <th>Bullet3</th>
      <th>Bullet4</th>
      <th>Bullet5</th>
      <th>Sku</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tab1s.each do |tab1| %>
      <tr>
        <td><%= tab1.productname %></td>
        <td><%= tab1.productparagraph %></td>
        <td><%= tab1.bullet1 %></td>
        <td><%= tab1.bullet2 %></td>
        <td><%= tab1.bullet3 %></td>
        <td><%= tab1.bullet4 %></td>
        <td><%= tab1.bullet5 %></td>
        <td><%= tab1.sku %></td>
        <td><%= link_to 'Show', tab1 %></td>
        <td><%= link_to 'Edit', edit_tab1_path(tab1) %></td>
        <td><%= link_to 'Destroy', tab1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tab1', new_tab1_path %>
